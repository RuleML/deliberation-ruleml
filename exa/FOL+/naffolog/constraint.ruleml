<?xml version="1.0" encoding="UTF-8"?>

<RuleML
xmlns="http://www.ruleml.org/spec"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.ruleml.org/1.0/xsd
http://www.ruleml.org/1.0/xsd/naffolog.xsd"
>

<Assert>

	<Entails>
	
		<Rulebase>
		    <!-- a particular watch is gold -->
		    <Atom>
		      <Rel>gold</Rel>
		      <Ind>watch17</Ind>
		    </Atom>
		
			<!-- inconsistent -->
		    <Atom>
		      <Rel>rusty</Rel>
		      <Ind>watch17</Ind>
		    </Atom>
			
		    <Atom>
		      <Rel>rusty</Rel>
		      <Reify>
		        <Implies material="yes">
		          <then>
		            <Atom><Rel>rusty</Rel><Var>object</Var></Atom>
		          </then>
		          <if>
		            <And>
		              <Atom><Rel>wet</Rel><Var>object</Var></Atom>
		              <Atom><Rel>metal</Rel><Var>object</Var></Atom>
		            </And>
		          </if>
		        </Implies>
		      </Reify>
		    </Atom>
			
		    <Atom>
		      <Rel>rusty</Rel> 
		      <Ind>car4</Ind>
		    </Atom>
		</Rulebase>
		
		<Rulebase>
		    <Neg>
		      <And>
		        <Atom>
		          <Rel>gold</Rel>
		          <Var>object</Var>
		        </Atom>
		        <Atom>
		          <Rel>rusty</Rel>
		          <Var>object</Var>
		        </Atom>       
		      </And>
		    </Neg>
		</Rulebase>
		
	</Entails>

</Assert>
	
</RuleML>